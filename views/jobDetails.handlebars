<h1>Add Job</h1>
{{#if error}}<span>{{error}}</span>{{/if}}
<form method="POST" action="submitted" enctype="multipart/form-data">
    <br>
    <div class="form-group">
        <label>Ticket Id column name</label>
        <br>
        <select id="ticket_id_column" name="ticket_id_column" aria-placeholder="Select ticket Id column name" aria-required="true">
            {{#each header}}
            <option value="{{this}}">{{this}}</option>
            {{/each}}
        </select>
    </div>
    <br>
    <div class="form-group">
        <label>API response code column name</label>
        <br>
        <select id="response_code" name="response_code" aria-placeholder="Select response code column name" aria-required="true">
            {{#each header}}
            <option value="{{this}}">{{this}}</option>
            {{/each}}
        </select>
    </div>
    <br>
    <div class="form-group">
        <label>API error message column name</label>
        <br>
        <select id="error_message" name="error_message" aria-placeholder="Select error message column name" aria-required="true">
            {{#each header}}
            <option value="{{this}}">{{this}}</option>
            {{/each}}
        </select>
    </div>

    <div class="form-group">
        <input type="hidden" class="form-control" name="sheet_ID" value="{{sheetID}}">
    </div>

    <div class="form-group">
        <input type="hidden" class="form-control" name="sheet_email" value="{{sheetClientEmail}}">
    </div>

    <div class="form-group">
        <input type="hidden" class="form-control" name="sheet_private_key" value="{{sheetPrivateKey}}">
    </div>

    <div class="form-group">
        <input type="hidden" class="form-control" name="api_key" value="{{apiKey}}">
    </div>

    <div class="form-group">
        <input type="hidden" class="form-control" name="fd_url" value="{{url}}">
    </div>

    {{#each header}}
    <div class="form-group">
        <input type="hidden" class="form-control" name="header[]" value="{{this}}">
    </div>
    {{/each}}

    
    
    <br>
    <br>
    <input type="submit" class="btn btn-primary" value="Save">
</form>
<script>
</script>